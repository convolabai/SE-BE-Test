{
  "name": "support-engineer-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "deploy": "docker compose up",
    "start": "yarn start:question1 && yarn start:question2",
    "start:question1": "node src/question-1/index.js",
    "start:question2": "node src/question-2/index.js",
    "test": "jest --watchAll --coverage --debug --runInBand",
    "test:unit": "yarn test --testPathPattern=tests/unit",
    "test:unit:q1": "yarn test --testPathPattern=tests/unit/question-1",
    "test:unit:q2": "yarn test --testPathPattern=tests/unit/question-2",
    "test:integration": "yarn test --testPathPattern=tests/integration",
    "test:integration:q1": "yarn test --testPathPattern=tests/integration/question-1",
    "test:integration:q2": "yarn test --testPathPattern=tests/integration/question-2",
    "prettify": "prettier --config .prettierrc \"src/**/*.js\" \"tests/**/*.js\" --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "auto-load": "^3.0.4",
    "dotenv": "^14.2.0",
    "mongoose": "^5.13.14"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^29.3.1",
    "prettier": "^2.8.1"
  }
}
